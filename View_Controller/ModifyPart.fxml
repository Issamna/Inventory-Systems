      <TextField fx:id="maxField" layoutX="101.0" layoutY="291.0" prefHeight="40.0" prefWidth="102.0" />
      <Label fx:id="minLabel" layoutX="232.0" layoutY="300.0" text="Min">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
