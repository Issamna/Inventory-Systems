      </Label>
      <Label fx:id="nameLabel" layoutX="50.0" layoutY="142.0" text="Name">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="174.0" layoutY="132.0" prefHeight="40.0" prefWidth="214.0"/>
