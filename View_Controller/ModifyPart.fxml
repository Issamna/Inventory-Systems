      <Button fx:id="saveBTN" onAction="#onActionSave" layoutX="234.0" layoutY="408.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Save" />
      <Button fx:id="cancelBTN" onAction="#onActionCancel" layoutX="316.0" layoutY="408.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Cancel" />
      <Label fx:id="idField" alignment="CENTER" layoutX="174.0" layoutY="80.0" prefHeight="40.0" prefWidth="214.0" style="-fx-border-width: 1; -fx-border-color: black; -fx-border-style: solid; -fx-padding: 5;">
