      <Label fx:id="priceLabel" layoutX="49.0" layoutY="245.0" text="Price/Cost">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="priceField" layoutX="174.0" layoutY="235.0" prefHeight="40.0" prefWidth="214.0" />
      <Label fx:id="maxLabel" layoutX="48.0" layoutY="300.0" text="Max">
         <font>
            <Font name="System Bold" size="14.0" />
